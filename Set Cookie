//Custom HTML Tag
<script type="text/javascript">
function setCookie(mktportal, tradedoubler, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = mktportal + "=" + tradedoubler + "; " + expires;
}
console.log("marketing portal set")
</script>
