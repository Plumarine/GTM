//Order ID
function () {
    var orderID_scrap = document.getElementsByClassName("resNumber")[0].innerHTML.slice(13,document.getElementsByClassName("resNumber")[0].innerHTML.length);
    window.param = "order ID="+orderID_scrap;   
    return orderID_scrap;
}
