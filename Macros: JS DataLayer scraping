//Accomodation name
function () {
    var product_name = ETB.analytics.params["accommodation_name"];
    window.param = "product_name="+product_name;   
    return product_name;
}

//Language
function () {
    var lang = ETB.analytics.params["lang"];
    window.param = "lang="+lang;   
    return lang;
}

//Revenue percentage
function () {
    var pteur = ETB.analytics.params["pteur"];
    var price = pteur*10/1000
    window.param = "price="+price;   
    return price;
}
